    "laravel-vite-plugin": "^1.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.6",
    "tslib": "^2.8.1",
    "typescript": "~5.8.0",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.4"
